{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "modules": false
    }
  },
  "rules": {
    "arrow-body-style": 2,
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "block-spacing": 2,
    "brace-style": [2, "1tbs", {
      "allowSingleLine": false
    }],
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "dot-notation": 2,
    "eol-last": 2,
    "indent": [2, 4, {
      "SwitchCase": 1
    }],
    "key-spacing": 2,
    "multiline-comment-style": 2,
    "no-confusing-arrow": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-else-return": 2,
    "no-extra-semi": 2,
    "no-lonely-if": 2,
    "no-mixed-operators": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": [2, {
      "max": 1,
      "maxEOF": 1
    }],
    "no-trailing-spaces": 2,
    "no-unsafe-negation": 2,
    "no-unused-vars": 1,
    "no-warning-comments": 1,
    "no-whitespace-before-property": 2,
    "no-var": 2,
    "one-var": [2, {
      "uninitialized": "always",
      "initialized": "never"
    }],
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-template": 2,
    "quote-props": [2, "as-needed"],
    "semi": 2,
    "semi-spacing": 2,
    "semi-style": 2,
    "template-curly-spacing": 2,
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "strict": 2,
    "valid-jsdoc": [2, {
      "requireReturn": false
    }]
  }
}